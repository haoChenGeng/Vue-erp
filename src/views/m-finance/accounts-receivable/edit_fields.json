           		{ "min": 1, "max": 128, "message": "注册地址长度不能超过128个字符", "trigger": "blur" }
           ],
           "companyCertificationUrl":[
           		{"required": true, "message": "请上传一般纳税人公司证明"}
           ],
           "customerInvoiceInfoUrl":[
           		{"required": true, "message": "请上传公司客户开票信息表"}
           ],
           "businessLicenceUrl":[
           		{"required": true, "message": "请上传公司营业执照复印件"}
