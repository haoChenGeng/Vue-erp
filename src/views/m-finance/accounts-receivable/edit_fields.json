	"rules":{
           "invoiceType":[
           		{"type":"number", "required": true, "message": "请选择开票类型", "trigger": "change"}
           ],
           "taxPayerType":[
           		{"type":"number", "required": true, "message": "请选择纳税人类型", "trigger": "change"}
           ],
           "invoiceTitle":[
           	    {"required": true, "message": "请填写发票抬头", "trigger": "blur"},
